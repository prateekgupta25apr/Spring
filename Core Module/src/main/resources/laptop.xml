<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="laptop" class="Laptop.Laptop" autowire="byName"/>
    <bean id="osName" class="Laptop.OS" autowire="constructor"/>

    <bean id="brand" class="java.lang.String">
        <constructor-arg value="Windows"/>
    </bean>
    <bean id="brandName" class="java.lang.String">
        <constructor-arg value="Lenovo"/>
    </bean>

    <bean id="version" class="java.lang.Integer">
        <constructor-arg value="25"/>
    </bean>


</beans>